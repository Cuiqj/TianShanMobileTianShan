        <pdf:text point="170,75">市（县）</pdf:text>
        
		<pdf:line fromPoint="25,87" toPoint="52,87" lineWidth="0.8"/>
		<pdf:text point="26,82" maxWidth="26" static="NO" fontSize="10">{{ agency }}</pdf:text>
        <pdf:text point="52,81">依法调查核实:</pdf:text>
		
