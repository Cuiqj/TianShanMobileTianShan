		<pdf:paragraph rect="{27,131},{158,44}" indent="head:75" lineHeight="8"  static="NO" fontSize="10">{{ payReason1 }}</pdf:paragraph>
		<pdf:paragraph rect="{27,163},{158,44}" lineHeight="8"  static="NO" fontSize="9">{{ payReason2 }}</pdf:paragraph>
