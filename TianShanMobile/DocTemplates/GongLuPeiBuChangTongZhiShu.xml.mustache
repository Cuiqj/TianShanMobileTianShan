    <pdf:content>
        <pdf:text point="75,50" fontName="SimHei" fontSize="17">公路赔（补）偿通知书</pdf:text>
        
        <pdf:text point="65,61.5">（</pdf:text>
        <pdf:text point="69,61.5" static="NO">{{ caseMark2 }}</pdf:text>
        <pdf:text point="79,61.5">）年</pdf:text>
        <pdf:text point="89.5,61.5" static="NO">{{ casePrefix }}</pdf:text>
        <pdf:text point="106,61.5">交赔字第</pdf:text>
        <pdf:text point="128,61.5" maxWidth="20" static="NO">{{ caseMark3 }}</pdf:text>
        <pdf:text point="138,61.5">号</pdf:text>
        
        <pdf:line fromPoint="22,78" toPoint="193,78" lineWidth="0.8"/>
        
        <pdf:text point="23,83">当  事  人:</pdf:text>
        <pdf:line fromPoint="54,88" toPoint="95,88" lineWidth="0.5"/>
        <pdf:text point="55,84" static="NO" fontSize="10">{{ partyName }}</pdf:text>
        
        <pdf:text point="23,93">当事人地址:</pdf:text>
        <pdf:line fromPoint="54,98" toPoint="160,98" lineWidth="0.5"/>
        <pdf:text point="55,94" maxWidth="90" static="NO" fontSize="10">{{ partyAddress }}</pdf:text>
        
        <pdf:text point="34,103">当事人</pdf:text>
        <pdf:line fromPoint="50,108" toPoint="98,108" lineWidth="0.5"/>
