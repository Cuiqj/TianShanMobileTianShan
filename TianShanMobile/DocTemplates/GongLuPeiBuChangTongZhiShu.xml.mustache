        <pdf:text point="117,101.5" maxWidth="42" static="NO" fontSize="10">{{ agencyCity }}</pdf:text>
        <pdf:text point="117,105" maxWidth="36" static="NO" fontSize="10">{{ agency }}</pdf:text>
        <pdf:text point="152,103">依法调查核实:</pdf:text>
        
        
        <pdf:text point="34,115">当事人</pdf:text>
        <pdf:line fromPoint="48,119.5" toPoint="75,119.5" lineWidth="0.5"/>
        <pdf:text point="50,115.5" static="NO" fontSize="10">{{ partyName }}</pdf:text>
        <pdf:text point="75,115">于</pdf:text>
